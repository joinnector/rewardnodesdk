{
	"name": "rewardnodesdk",
	"version": "1.0.0",
	"description": "ester rewardnodesdk",
	"author": "ester",
	"husky": {
		"hooks": {
			"pre-commit": "yarn run ts:lint && yarn run js:lint && yarn build",
			"pre-push": "yarn run ts:lint && yarn run js:lint && yarn build"
		}
	},
	"scripts": {
		"build": "rm -rf dist && tsc",
		"ts:lint": "eslint . --ext .ts",
		"ts:lint:fix": "eslint . --ext .ts --fix",
		"js:lint": "eslint . --ext .js",
		"js:lint:fix": "eslint . --ext .js --fix",
		"console": "npx ./ecosystem.console.js"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"lodash": "^4.17.20",
		"moment": "^2.27.0",
		"querystring": "^0.2.0",
		"uuid": "^8.2.0",
		"uuid-validate": "^0.0.3",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.11.5",
		"@babel/runtime": "^7.11.2",
		"@types/lodash": "^4.14.157",
		"@types/node": "^14.0.1",
		"@types/string-template": "^1.0.2",
		"@types/uuid": "^8.0.0",
		"@types/uuid-validate": "^0.0.1",
		"@typescript-eslint/eslint-plugin": "^3.6.0",
		"@typescript-eslint/parser": "^3.6.0",
		"eslint": "6.0.0",
		"husky": "^4.3.0",
		"typescript": "^3.9.2"
	}
}
